{
  "kind": "singleType",
  "collectionName": "blog_pages",
  "info": {
    "singularName": "blog-page",
    "pluralName": "blog-pages",
    "displayName": "BlogPage"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "default": "our blogs",
      "private": true,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "pageinfo": {
      "type": "component",
      "repeatable": true,
      "component": "layout.page-info"
    },
    "excludedTags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tag.tag"
    }
  }
}
